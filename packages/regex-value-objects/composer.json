{
  "name": "apie/regex-value-objects",
  "description": "Composer package of the apie library: regex value objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\RegexValueObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\RegexValueObjects\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "phpunit/phpunit": "^9.6.19",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "extra": {
    "apie-objects": [
      "Apie\\RegexValueObjects\\PhpRegularExpression",
      "Apie\\RegexValueObjects\\PhpSafeRegularExpression"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}