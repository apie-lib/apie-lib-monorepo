<apie-form-field-definition
  name="{{ property('name').getChildFormFieldName() }}"
  types="select"
  label="{{ property('name').getChildFormFieldName() }}"
  class="unhandled-select">
</apie-form-field-definition>
<apie-script>
(function(elm) {
  elm.classList.remove('unhandled-select');
  elm.additionalSettings = {
    options: {{ property('options') | json_encode | replace({'<': '&lt;'}) | raw }}
  }
}(document.querySelector('apie-form-field-definition.unhandled-select')));
</apie-script>