{
  "name": "apie/apie-bundle",
  "description": "Composer package of the apie library: apie bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\ApieBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\ApieBundle\\": "tests/"
    }
  },
  "suggest": {
    "apie/cms": "Access domain object actions in a fully working admin panel to your application",
    "apie/console": "Access domain object actions by symfony console",
    "apie/doctrine-entity-datalayer": "Store domain objects by using generated doctrine entities internally",
    "apie/rest-api": "Access domain object actions in a fully working REST API to your application"
  },
  "require": {
    "php": ">=8.3",
    "apie/common": "self.version",
    "apie/core": "self.version",
    "symfony/config": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/finder": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/psr-http-message-bridge": "^7.2",
    "symfony/routing": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2",
    "apie/cms": "self.version",
    "apie/cms-layout-graphite": "self.version",
    "apie/console": "self.version",
    "apie/country-and-phone-number": "self.version",
    "apie/faker": "self.version",
    "apie/fixtures": "self.version",
    "apie/text-value-objects": "self.version",
    "apie/rest-api": "self.version",
    "doctrine/persistence": "^3.1.0",
    "league/openapi-psr7-validator": "0.22",
    "symfony/framework-bundle": "^7.2",
    "symfony/security-bundle": "^7.2",
    "symfony/twig-bundle": "^7.2",
    "symfony/validator": "^7.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}