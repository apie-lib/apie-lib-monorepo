{
  "name": "apie/apie-bundle",
  "description": "Composer package of the apie library: apie bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\ApieBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\ApieBundle\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "symfony/config": "6.*",
    "symfony/dependency-injection": "6.*",
    "symfony/finder": "6.*",
    "symfony/http-kernel": "6.*",
    "symfony/psr-http-message-bridge": "2.*",
    "symfony/routing": "6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0",
    "apie/rest-api": "self.version",
    "symfony/framework-bundle": "6.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}