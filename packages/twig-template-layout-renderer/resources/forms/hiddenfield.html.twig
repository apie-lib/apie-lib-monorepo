<apie-form-field-definition {% for key, value in property('additionalAttributes') %}
  {{ key }}="{{ value }}"
{% endfor %}
  name="{{ property('name').getChildFormFieldName() }}"
  types="hidden"
  label="{{ property('name').getChildFormFieldName() }}"
></apie-form-field-definition>
{{ renderValidationError(property('name').getChildFormFieldName(), property('value'), property('validationError')) }}
