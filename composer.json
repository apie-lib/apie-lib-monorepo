{
    "name": "apie/apie-lib",
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter_jordaan@hotmail.com"
        }
    ],
    "description": "Monorepo of the Apie library",
    "type": "project",
    "license": "MIT",
    "require": {
        "apie/phpunit-matrix-data-provider": "^1.0.2",
        "apie/service-provider-generator": "0.10.1",
        "apie/type-converter": "^1.0.2",
        "cebe/php-openapi": "^1.7",
        "doctrine/common": "^3.4.3",
        "doctrine/orm": "^2.16",
        "doctrine/persistence": "^3.2",
        "egulias/email-validator": "^3.2.1",
        "ext-mbstring": "*",
        "fakerphp/faker": "^1.19",
        "giggsey/libphonenumber-for-php": "^8.0",
        "laravel/laravel": "7.*|8.*|9.*|10.*",
        "nette/php-generator": "^4.0.1",
        "nikic/php-parser": "^4.14",
        "niklongstone/regex-reverse": "^0.4.0",
        "nyholm/psr7": "^1.5",
        "php": ">=8.1",
        "prinsfrank/standards": "^1.0.0",
        "psr/http-message": "^1.0",
        "ramsey/uuid": "^4.3.1",
        "symfony/config": "6.*",
        "symfony/console": "6.*",
        "symfony/dependency-injection": "6.*",
        "symfony/event-dispatcher": "6.*",
        "symfony/finder": "6.*",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "6.*",
        "symfony/mime": "6.*",
        "symfony/psr-http-message-bridge": "2.*",
        "symfony/routing": "6.*",
        "twig/twig": "^3.4.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "friendsofphp/php-cs-fixer": "^3.8",
        "league/openapi-psr7-validator": "0.18",
        "orchestra/testbench": "^8.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.8.2",
        "phpunit/phpunit": "^9.5",
        "symfony/cache": "^6.0",
        "symfony/framework-bundle": "6.*",
        "symfony/phpunit-bridge": "6.*",
        "symfony/security-bundle": "6.*",
        "symfony/twig-bundle": "6.*",
        "symplify/monorepo-builder": "10.2.7"
    },
    "autoload": {
        "psr-4": {
            "Apie\\ApieBundle\\": "packages/apie-bundle/src/",
            "Apie\\ApiePhpstanRules\\": "packages/apie-phpstan-rules/src/",
            "Apie\\CmsApiDropdownOption\\": "packages/cms-api-dropdown-option/src/",
            "Apie\\CmsLayoutGraphite\\": "packages/cms-layout-graphite/src/",
            "Apie\\Cms\\": "packages/cms/src/",
            "Apie\\CommonValueObjects\\": "packages/common-value-objects/src/",
            "Apie\\Common\\": "packages/common/src/",
            "Apie\\CompositeValueObjects\\": "packages/composite-value-objects/src/",
            "Apie\\Console\\": "packages/console/src/",
            "Apie\\Core\\": "packages/core/src/",
            "Apie\\CountWords\\": "packages/count-words/src/",
            "Apie\\CountryAndPhoneNumber\\": "packages/country-and-phone-number/src/",
            "Apie\\DateValueObjects\\": "packages/date-value-objects/src/",
            "Apie\\DateformatToRegex\\": "packages/dateformat-to-regex/src/",
            "Apie\\DoctrineEntityConverter\\": "packages/doctrine-entity-converter/src/",
            "Apie\\DoctrineEntityDatalayer\\": "packages/doctrine-entity-datalayer/src/",
            "Apie\\Faker\\": "packages/faker/src/",
            "Apie\\Fixtures\\": "packages/fixtures/src/",
            "Apie\\HtmlBuilders\\": "packages/html-builders/src/",
            "Apie\\IntegrationTests\\": "packages/integration-tests/src/",
            "Apie\\LaravelApie\\": "packages/laravel-apie/src/",
            "Apie\\RegexValueObjects\\": "packages/regex-value-objects/src/",
            "Apie\\RestApi\\": "packages/rest-api/src/",
            "Apie\\SchemaGenerator\\": "packages/schema-generator/src/",
            "Apie\\Serializer\\": "packages/serializer/src/",
            "Apie\\TextValueObjects\\": "packages/text-value-objects/src/",
            "Apie\\Tools\\": "tools/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apie\\Tests\\ApieBundle\\": "packages/apie-bundle/tests/",
            "Apie\\Tests\\ApiePhpstanRules\\": "packages/apie-phpstan-rules/tests/",
            "Apie\\Tests\\CmsApiDropdownOption\\": "packages/cms-api-dropdown-option/tests/",
            "Apie\\Tests\\CmsLayoutGraphite\\": "packages/cms-layout-graphite/tests/",
            "Apie\\Tests\\Cms\\": "packages/cms/tests/",
            "Apie\\Tests\\CommonValueObjects\\": "packages/common-value-objects/tests/",
            "Apie\\Tests\\Common\\": "packages/common/tests/",
            "Apie\\Tests\\CompositeValueObjects\\": "packages/composite-value-objects/tests/",
            "Apie\\Tests\\Console\\": "packages/console/tests/",
            "Apie\\Tests\\Core\\": "packages/core/tests/",
            "Apie\\Tests\\CountWords\\": "packages/count-words/tests/",
            "Apie\\Tests\\CountryAndPhoneNumber\\": "packages/country-and-phone-number/tests/",
            "Apie\\Tests\\DateValueObjects\\": "packages/date-value-objects/tests/",
            "Apie\\Tests\\DateformatToRegex\\": "packages/dateformat-to-regex/tests/",
            "Apie\\Tests\\DoctrineEntityConverter\\": "packages/doctrine-entity-converter/tests/",
            "Apie\\Tests\\DoctrineEntityDatalayer\\": "packages/doctrine-entity-datalayer/tests/",
            "Apie\\Tests\\Faker\\": "packages/faker/tests/",
            "Apie\\Tests\\Fixtures\\": "packages/fixtures/tests/",
            "Apie\\Tests\\HtmlBuilders\\": "packages/html-builders/tests/",
            "Apie\\Tests\\LaravelApie\\": "packages/laravel-apie/tests/",
            "Apie\\Tests\\RegexValueObjects\\": "packages/regex-value-objects/tests/",
            "Apie\\Tests\\RestApi\\": "packages/rest-api/tests/",
            "Apie\\Tests\\SchemaGenerator\\": "packages/schema-generator/tests/",
            "Apie\\Tests\\Serializer\\": "packages/serializer/tests/",
            "Apie\\Tests\\TextValueObjects\\": "packages/text-value-objects/tests/"
        }
    },
    "config": {
        "lock": false
    },
    "replace": {
        "apie/apie-bundle": "self.version",
        "apie/apie-phpstan-rules": "self.version",
        "apie/cms": "self.version",
        "apie/cms-api-dropdown-option": "self.version",
        "apie/cms-layout-graphite": "self.version",
        "apie/common": "self.version",
        "apie/common-value-objects": "self.version",
        "apie/composite-value-object": "self.version",
        "apie/composite-value-objects": "self.version",
        "apie/console": "self.version",
        "apie/core": "*",
        "apie/count-words": "self.version",
        "apie/country-and-phone-number": "self.version",
        "apie/date-value-objects": "self.version",
        "apie/dateformat-to-regex": "self.version",
        "apie/doctrine-entity-converter": "self.version",
        "apie/doctrine-entity-datalayer": "self.version",
        "apie/doctrine-metadata-driver": "self.version",
        "apie/faker": "self.version",
        "apie/fixtures": "self.version",
        "apie/html-builders": "self.version",
        "apie/laravel-apie": "self.version",
        "apie/regex-value-objects": "self.version",
        "apie/rest-api": "self.version",
        "apie/schema-generator": "self.version",
        "apie/serializer": "self.version",
        "apie/text-value-objects": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
